<Page x:Class="ProjekatWpfHci.Anamneza"      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"       xmlns:d="http://schemas.microsoft.com/expression/blend/2008"       xmlns:local="clr-namespace:ProjekatWpfHci"      mc:Ignorable="d"       d:DesignHeight="450" d:DesignWidth="800"      Title="Anamneza" Background="White">    <Grid Margin="0 30 0 0">        <Grid.ColumnDefinitions>            <ColumnDefinition Width="*"></ColumnDefinition>            <ColumnDefinition Width="*"></ColumnDefinition>        </Grid.ColumnDefinitions>        <StackPanel Grid.Column="0">            <Label FontSize="25" Margin="40 10 0 0 "   FontWeight="Medium" FontFamily="Calibri">Glavne tegobe: </Label>            <Label FontSize="25" Margin="40 10 0 0 "   FontWeight="Medium" FontFamily="Calibri">Sadašnja bolest</Label>            <Label FontSize="20" Margin="40 10 0 0 "   FontFamily="Calibri">Kada je počela: </Label>            <Label FontSize="20" Margin="40 10 0 0 "   FontFamily="Calibri">Kako je počela(naglo ili postepeno):</Label>            <Label FontSize="20" Margin="40 10 0 0 "   FontFamily="Calibri">Sa kojim simptomima je počela:</Label>            <Label FontSize="20" Margin="40 10 0 0 "   FontFamily="Calibri">Da li je dobio/la terapiju i koju:</Label>            <Label FontSize="25" Margin="40 10 0 0 "   FontFamily="Calibri" FontWeight="Medium">Opšte tegobe</Label>            <Label FontSize="20" Margin="40 10 0 0 "   FontFamily="Calibri">Temperatura(visina, dužina trajanja):</Label>            <Label FontSize="20" Margin="40 10 0 0 "   FontFamily="Calibri">Malaksalost:</Label>            <Label FontSize="20" Margin="40 10 0 0 "   FontFamily="Calibri">Promene u težini:</Label>        </StackPanel>        <StackPanel x:Name="anamnezaForma" Grid.Column="1">            <TextBox x:Name="text1" Margin="0 05 80 0" FontWeight="Medium" Width="300" Height="40" BorderBrush="Turquoise" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" ></TextBox>            <Label FontSize="15" Margin="10 33 0 0 "></Label>            <TextBox x:Name="text2" BorderBrush="Turquoise" Margin="0 20 80 0" FontWeight="Medium" Width="300" Height="30" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></TextBox>            <StackPanel Margin="90 20 0 0" Orientation="Horizontal">                <RadioButton  x:Name="Nema" FontFamily="Calibri" FontSize="15" Margin="20 0 20 0">Naglo</RadioButton>                <RadioButton x:Name="Me" FontFamily="Calibri" FontSize="15" >Postepeno</RadioButton>            </StackPanel>            <TextBox  x:Name="text3" BorderBrush="Turquoise" Margin="0 5 80 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"  FontWeight="Medium" Width="300" Height="30"></TextBox>            <TextBox  x:Name="text4" BorderBrush="Turquoise" Margin="0 18 80 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"  FontWeight="Medium" Width="300" Height="30"></TextBox>            <TextBox  x:Name="text5" BorderBrush="Turquoise" Margin="0 64 80 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"  FontWeight="Medium" Width="300" Height="30"></TextBox>            <TextBox  x:Name="text6" BorderBrush="Turquoise" Margin="0 15 80 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"  FontWeight="Medium" Width="300" Height="30"></TextBox>            <TextBox  x:Name="text7" BorderBrush="Turquoise" Margin="0 10 80 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"  FontWeight="Medium" Width="300" Height="30"></TextBox>            <StackPanel Orientation="Horizontal">                <Button Margin="120 50 10 0" Content="Save" Click="saveClick" Width="80" Height="30"></Button>                <Button Margin="0 50 0 0" Content="Discard" Background="Red" Click="discardClick" Width="80" Height="30"></Button>             </StackPanel>        </StackPanel>    </Grid></Page>